{
  "kind": "collectionType",
  "collectionName": "section_resources",
  "info": {
    "singularName": "section-resource",
    "pluralName": "section-resources",
    "displayName": "Section Resource",
    "description": "Join table for Section and Resource with ordering"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order": {
      "type": "integer",
      "required": true
    },
    "section": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::section.section",
      "inversedBy": "section_resources"
    },
    "resource": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::resource.resource",
      "inversedBy": "section_resources"
    }
  }
} 